FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     detonationSprayFoam;//rhoCentralFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         5e-4;

deltaT          1e-10;

writeControl    adjustableRunTime;

writeInterval   5e-6;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.1;

maxDeltaT       1;

