FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 301
    volScalarFieldValue p 101325
);

regions
(
    boxToCell
    {
        box (0 0 0) (0.008 0.001 0.00236);
        fieldValues
        (
            volScalarFieldValue C7H16 0.0621
            volScalarFieldValue N2 0.7193
            volScalarFieldValue O2 0.2186
            volScalarFieldValue T 3000
            volScalarFieldValue p 9119250
        );
    }
);


